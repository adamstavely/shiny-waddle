    "template": {
      "name": "Department Access Policy",
      "description": "Controls access based on user department",
      "type": "rbac",
      "version": "1.0.0",
      "status": "draft",
      "rules": [
        {
          "id": "department-match",
          "description": "Allow access if user department matches resource department",
          "effect": "allow",
          "conditions": {
            "subject.department": "{{resource.department}}"
          }
        }
      ]
