  {
    "id": "policy-002",
    "name": "Data Classification Access Control",
    "description": "Controls access based on data classification levels and user clearance",
    "type": "abac",
    "version": "1.3.0",
    "status": "active",
    "effect": "allow",
    "priority": 150,
    "conditions": [
      {
        "attribute": "subject.clearanceLevel",
        "operator": "greaterThan",
        "value": "{{resource.dataClassification}}",
        "logicalOperator": ""
      },
      {
        "attribute": "subject.department",
        "operator": "equals",
        "value": "{{resource.department}}",
        "logicalOperator": "AND"
      },
      {
        "attribute": "context.location",
        "operator": "in",
        "value": "['US', 'CA', 'UK']",
        "logicalOperator": "AND"
      }
    ],
