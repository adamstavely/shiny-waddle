  {
    "id": "policy-004",
    "name": "Time-Based Access Restriction",
    "description": "Restricts access during non-business hours and weekends",
    "type": "abac",
    "version": "1.1.0",
    "status": "active",
    "effect": "deny",
    "priority": 200,
    "conditions": [
      {
        "attribute": "context.timeOfDay",
        "operator": "lessThan",
        "value": "08:00",
        "logicalOperator": ""
      },
      {
        "attribute": "context.timeOfDay",
        "operator": "greaterThan",
        "value": "18:00",
        "logicalOperator": "OR"
      },
      {
        "attribute": "context.dayOfWeek",
        "operator": "in",
