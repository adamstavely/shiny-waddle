    "id": "policy-001",
    "name": "Admin Full Access Policy",
    "description": "Grants full administrative access to users with admin role",
    "type": "rbac",
    "version": "2.1.0",
    "status": "active",
    "effect": "allow",
    "priority": 100,
    "rules": [
      {
        "id": "admin-full-access",
        "description": "Full access for administrators",
        "effect": "allow",
        "conditions": {
          "subject.role": ["admin", "super-admin"]
        }
      },
      {
        "id": "admin-read-only",
        "description": "Read-only access for admin viewers",
        "effect": "allow",
