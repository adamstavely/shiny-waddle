  {
    "id": "policy-007",
    "name": "IP-Based Access Control",
    "description": "Restricts access based on IP address ranges",
    "type": "abac",
    "version": "1.2.0",
    "status": "active",
    "effect": "allow",
    "priority": 90,
    "conditions": [
      {
        "attribute": "context.ipAddress",
