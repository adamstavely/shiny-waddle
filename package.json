{
  "name": "@research-tracker/heimdall",
  "version": "1.0.0",
  "description": "Heimdall - Data Access Testing Framework for compliance verification",
  "main": "dist/index.js",
  "bin": {
    "heimdall": "./heimdall-framework/cli/index.ts"
  },
  "scripts": {
    "test:compliance": "ts-node heimdall-framework/src/run-tests.ts",
    "test:compliance:watch": "ts-node --watch heimdall-framework/src/run-tests.ts",
    "dashboard:api": "cd dashboard-api && npm run start:dev",
    "dashboard:frontend": "cd dashboard-frontend && npm run dev",
    "dashboard:install": "cd dashboard-api && npm install && cd ../dashboard-frontend && npm install",
    "dashboard:start": "./start-local.sh",
    "build": "tsc",
    "lint": "eslint heimdall-framework/src --ext .ts"
  },
  "keywords": [
    "compliance",
    "testing",
    "access-control",
    "data-governance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.13.2",
    "commander": "^11.0.0",
    "express": "^4.22.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "chokidar": "^3.5.3",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.0"
  }
}
